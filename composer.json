{
    "name": "zazama/analytics",
    "type": "silverstripe-vendormodule",
    "description": "A privacy focused analytics module integrated into Silverstripe",
    "keywords": [
        "silverstripe",
        "analytics"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Jan Metzger",
            "homepage": "https://github.com/Zazama"
        }
    ],
    "require": {
        "silverstripe/framework": "^4.4",
        "silverstripe/admin": "^1",
        "silverstripe/vendor-plugin": "^1",
        "matomo/device-detector": "^4.3",
        "defuse/php-encryption": "^2.3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3"
    },
    "autoload": {
        "psr-4": {
            "Zazama\\Analytics\\": "src/",
            "Zazama\\Analytics\\Tests\\": "tests/"
        }
    },
    "extra": {
        "expose": [
            "client/dist"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
